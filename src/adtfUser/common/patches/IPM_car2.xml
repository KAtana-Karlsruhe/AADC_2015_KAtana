<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!-- Four measured points in world and in camera image-->
<ipm_calibration>
	<pair id="up-left">
		<!-- describes the measured pixel coordinates in camera image-->
		<camera_in xvalue="172" yvalue="260"/>
		<!-- describes the measured distance in the world in 1/10mm-->
		<camera_out xvalue="25040" yvalue="6020"/>
	</pair>
	<pair id="up-right">
		<!-- describes the measured pixel coordinates in camera image-->
		<camera_in xvalue="464" yvalue="258"/>
		<!-- describes the measured distance in the world in 1/10mm-->
		<camera_out xvalue="25040" yvalue="-6030"/>
	</pair>
	<pair id="down-right">
		<!-- describes the measured pixel coordinates in camera image-->
		<camera_in xvalue="475" yvalue="369"/>
		<!-- describes the measured distance in the world in 1/10mm-->
		<camera_out xvalue="10030" yvalue="-2000"/>
	</pair>
	<pair id="down-left">
		<!-- describes the measured pixel coordinates in camera image-->
		<camera_in xvalue="176" yvalue="371"/>
		<!-- describes the measured distance in the world in 1/10mm-->
		<camera_out xvalue="10030" yvalue="2000"/>
	</pair>

	<!-- specify the size (in 1mm/10) of the resulting picture -->
	<size width="64666.667" height="62666.67"/>
</ipm_calibration>
<pixel_conversion>
	<!-- describes the pixel to 1/10mm ratio-->        
	<pixelRatio value="0.01"/>
	<!-- pixel coordinate origin in car coordinates-->
	<pixel_coordinate_origin xvalue="50000" yvalue="32333.33"/>
</pixel_conversion>
